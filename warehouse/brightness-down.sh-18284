#!/bin/bash

CURRENT_BRIGHTNESS=$(cat "/sys/devices/platform/s3c24xx-pwm.0/pwm-backlight.0/backlight/pwm-backlight.0/brightness");
NEW_BRIGHTNESS=$(expr $CURRENT_BRIGHTNESS - 25);
echo "Current: $CURRENT_BRIGHTNESS";
echo "New: $NEW_BRIGHTNESS";
echo "$NEW_BRIGHTNESS" > "/sys/devices/platform/s3c24xx-pwm.0/pwm-backlight.0/backlight/pwm-backlight.0/brightness";
